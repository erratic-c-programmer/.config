{
	"rust-analyzer.serverPath": "/usr/bin/rust-analyzer",
	"coc.preferences.formatOnType": true,
	"rust-analyzer.diagnostics.enableExperimental": true,
	"suggest.echodocSupport": true,
	"languageserver": {
		"haskell": {
			"command": "ghcide",
			"args": [
				"--lsp"
			],
			"rootPatterns": [
				".stack.yaml",
				".hie-bios",
				"BUILD.bazel",
				"cabal.config",
				"package.yaml"
			],
			"filetypes": [
				"hs",
				"lhs",
				"haskell"
			]
		}
	}
}
